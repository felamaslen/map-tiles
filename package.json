{
  "name": "map-tiles",
  "version": "0.0.1",
  "description": "Mapping frontend with example tile generator",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --inspect -w ./app index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require babel-core/register \"test/**/*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felamaslen/map-tiles.git"
  },
  "author": "Fela Maslen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felamaslen/map-tiles/issues"
  },
  "homepage": "https://github.com/felamaslen/map-tiles#readme",
  "babel": {
    "env": {
      "production": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions",
                  "ie >= 10"
                ]
              }
            }
          ],
          "stage-0"
        ]
      },
      "development": {
        "presets": [
          "react",
          "stage-0"
        ],
        "sourceMaps": "inline"
      },
      "test": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "node": "8"
              }
            }
          ],
          "stage-0"
        ]
      }
    }
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "dotenv": "^5.0.0",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.1",
    "react-pure-render": "^1.0.2",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga-test-plan": "^3.5.0",
    "redux-test-utils": "^0.2.2",
    "style-loader": "^0.20.2",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.8",
    "html-webpack-plugin": "^2.30.1",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-saga": "^0.16.0",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-spritesmith": "^0.4.0"
  }
}
